<Application x:Class="WorkerHaCS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WorkerHaCS"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\TabControl.xaml" />
                <ResourceDictionary Source="Styles\AdditionalTabControl.xaml" />
                <ResourceDictionary Source="Styles\Grid.xaml" />
                <ResourceDictionary Source="Styles\DataGrid.xaml" />
                <ResourceDictionary Source="Styles\Button.xaml" />
                <ResourceDictionary Source="Styles\TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
